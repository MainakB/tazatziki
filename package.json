{
  "name": "tziki",
  "version": "1.0.0",
  "private": false,
  "description": "A test project scaffolding",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@wdio/cli": "^7.20.5",
    "@wdio/cucumber-framework": "^7.20.3",
    "@wdio/local-runner": "^7.20.5",
    "@wdio/selenium-standalone-service": "^7.20.3",
    "@wdio/spec-reporter": "^7.20.3",
    "chai": "^4.3.6",
    "ffmpeg": "^0.0.4",
    "multiple-cucumber-html-reporter": "^1.21.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "wdio-cucumberjs-json-reporter": "^4.4.1",
    "wdio-ms-teams-service": "^0.1.9",
    "wdio-video-reporter": "^3.2.1",
    "wdio-wait-for": "^2.2.6",
    "winston": "^3.8.1"
  },
  "scripts": {
    "prepare": "npm run compile:all",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "compile:all": "tsc --diagnostics",
    "clean:build": "rimraf dist",
    "clean:reports": "rimraf Reports",
    "clean:pretest": "run-p clean:build clean:reports",
    "compile": "run-s clean:pretest compile:all",
    "test": "wdio run src/confs/wdio.conf.ts",
    "clean:test": "run-s clean:pretest test",
    "compile:test": "run-s compile test"
  }
}
